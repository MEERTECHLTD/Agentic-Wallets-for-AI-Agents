# Dashboard
DASHBOARD_PORT=3000

# Solana Network Configuration
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com

# Agent Configuration
AGENT_COUNT=3
AGENT_DECISION_INTERVAL_MS=15000
AGENT_MAX_SOL_PER_TX=0.01

# Wallet Storage
WALLET_STORAGE_DIR=./wallets
ENCRYPT_WALLETS=true

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/agent.log

# Optional: Airdrop on wallet creation (devnet only)
AUTO_AIRDROP=true
AIRDROP_AMOUNT_SOL=1
